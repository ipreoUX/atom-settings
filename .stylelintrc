{
  "rules": {
    "at-rule-no-unknown": [ true, {
      "ignoreAtRules": [
        "include",
        "mixin",
        "each",
        "content",
        "else",
        "if"
      ]
    }],
    "number-leading-zero": null,
    "color-hex-case": "lower",
    "color-hex-length": "long",
    "unit-whitelist": ["em", "rem", "s", "%", "vh", "px"]
  }
}
